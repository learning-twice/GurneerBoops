{
  "expo": {
    "name": "GurneerBoops",
    "slug": "GurneerBoops",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "gurneerboops",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.gurneerorg.GurneerBoops",
      "associatedDomains": ["applinks:www.boopapp.site", "applinks:boopapp.site"]
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
      "expo-notifications",
      {
        "sounds": [
          "./assets/be_quiet.wav",
          "./assets/bye-bye.wav",
          "./assets/daddy.wav",
          "./assets/good_morning.wav",
          "./assets/haha.wav",
          "./assets/googleit.wav",
          "./assets/HiyaGeorgie.wav",
          "./assets/offer_x.wav",
          "./assets/questions.wav"

        ]
      }
    ],
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.895289514692-lpihg3tl9unlc3thtklej03e6ckru1rl"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "71678c13-854c-428b-8b78-d21e574cbdaf"
      }
    },
    "owner": "gurneerorg",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/71678c13-854c-428b-8b78-d21e574cbdaf",
       "requestHeaders": {
    "expo-channel-name": "production"
  },
  "checkAutomatically": "ON_LOAD",
  "fallbackToCacheTimeout": 0
    }
  }
}
